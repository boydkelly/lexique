	<item>
		<jula>àyiwa</jula>
		<french>
			<translations>
				<definition>eh bien, euh, bon</definition>
				<hint></hint>
				<speech>
					<type></type>
					<example>
						<jula>farignan k'a minɛ, ayiwa, kunasini sɔgɔma.</jula>
						<french>Il a commencé à avoir de la fièvre, ehu, avant-hier matin.</french>
					</example>
					<example>
						<jula>ayiwa, n be taga.</jula>
						<french>Bon, je m'en vais</french>
					</example>
					<example>
						<jula>ayiwa, a be mun lo kɛla ?</jula>
						<french>Eh bien, qu'est-ce qu'il fait ?</french>
					</example>
				</speech>
			</translations>
		</french>
