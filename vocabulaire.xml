	<item>
		<jula>baara</jula>
		<french>
			<translations>
				<definition>travail</definition>
				<hint></hint>
				<speech>
					<type>n</type>
					<example>
						<jula>i ta baara ka ɲi i ma wa ?</jula>
						<french>Est-ce que ton travail te satisfait ?</french>
					</example>
					<example>
						<jula>i ni barra</jula>
						<french>Bonjour/Bon travail !</french>
					</example>
				</speech>
			</translations>
			<translations>
				<definition>travailler</definition>
				<hint></hint>
				<speech>
					<type>vi</type>
					<example>
						<jula>i be baara kɛ</jula>
						<french>Il travaille</french>
					</example>
				</speech>
				<speech>
					<type>vt</type>
					<example>
						<jula>i be baara la</jula>
						<french>Il est au travail</french>
					</example>
					<example>
						<jula>a be nɛgɛ baara</jula>
						<french>il travaille le fer</french>
					</example>
				</speech>
			</translations>
		</french>
	</item>
