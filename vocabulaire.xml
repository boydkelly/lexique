	<item>
		<jula>bási</jula>
		<french>
			<translations>
				<definition>ennuis, chose grave</definition>
				<hint></hint>
				<speech>
					<type></type>
					<example>
						<jula>ji ma glasi kosobɛ. baasi tɛ, dɔ di n ma.</jula>
						<french>L'eau n'est pas très fraiche. Ça ne fait rien, donne-m'en.</french>
					</example>
					<example>
						<jula>sabali! baasi tɛ!</jula>
						<french>Pardon ! de rien !</french>
					</example>
					<example>
						<jula>taji banna. o kɛra baasi ye</jula>
						<french>L'essence est fini. C'est grave.</french>
					</example>
				</speech>
			</translations>
		</french>
	</item>
